{
  "expo": {
    "name": "MyEx",
    "slug": "myex-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myexapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F0F7FF",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "package": "com.nimsara.jayarathna.myexapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#F0F7FF"
        }
      ]
    ]
  }
}
